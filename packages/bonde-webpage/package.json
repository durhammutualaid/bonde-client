{
  "name": "bonde-webpage",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nossas/bonde-client.git"
  },
  "dependencies": {
    "axios": "^0.18.0",
