    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sinon": "^7.2.3"
  },
  "scripts": {
    "preinstall": "rm -f node_modules/@bonde-webpage",
    "postinstall": "ln -s `pwd`/src/lib node_modules/@bonde-webpage",
